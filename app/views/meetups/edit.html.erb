<%= form_tag "/client/meetups/#{@meetup["id"]}", method: :patch do %>
  <div>
    Name: <input type="text" name="name" value="<%= @meetup["name"] %>" />
  </div>
  <div>
    Start Time: <input type="text" name="start_time" value="<%= @meetup["start_time"] %>" />
  </div>
  <div>
    End Time: <input type="text" name="end_time" value="<%= @meetup["end_time"] %>" />
  </div>
 
  <input type="submit" name="" value="Update Meetup"/>
<% end %>